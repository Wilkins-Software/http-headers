{
  "version": "0.2.4",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint --fix -c .eslintrc.js src/**/*.ts",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish:np": "np  --no-yarn",
    "generate-barrels": "barrelsby --delete -d src -l all && npm run lint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@wilkins-software/http-headers",
  "author": "Mark Wilkins",
  "module": "dist/http-headers.esm.js",
  "size-limit": [
    {
      "path": "dist/http-headers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/http-headers.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "barrelsby": "^2.5.1",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "np": "^7.7.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/lodash": "^4.14.194",
    "@wilkins-software/time-conversion-helpers": "^0.2.2",
    "lodash": "^4.17.21"
  }
}